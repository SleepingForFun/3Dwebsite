<h1> Edit Customer</h1>
<form action="/customer/save" method="post">
<input type="hidden" name="customer_id" value="<%= onerec.customer_id %>" >
<table>

<tr> <td> Firstname: </td> <td> <input type="text" name="firstname" value="<%= onerec.firstname %>" required> </td> </tr>
<tr> <td> LastName: </td> <td> <input type="text" name="lastname" value="<%= onerec.lastname %>" required> </td> </tr>
<tr> <td> Phone: </td> <td> <input type="text" name="phone" value="<%= onerec.phone %>" required> </td> </tr>
<tr> <td> Email: </td> <td> <input type="text" name="email" value="<%= onerec.email %>" required> </td> </tr>
<tr> <td> Address: </td> <td> <input type="text" name="address1" value="<%= onerec.address1 %>" required> </td> </tr>

<tr> <td> City: </td> <td> <input type="text" name="city" value="<%= onerec.city %>" required> </td> </tr>
<tr> <td> State: </td> <td> <input type="text" name="state" value="<%= onerec.state %>" required> </td> </tr>
<tr> <td> Zip: </td> <td> <input type="text" name="zip" value="<%= onerec.zip %>" required> </td> </tr>
<tr> <td> User: </td> <td> <input type="text" name="username" value="<%= onerec.username %>" required> </td> </tr>
<tr> <td> password: </td> <td> <input type="text" name="password" value="<%= onerec.password %>" required> </td> </tr>
</table>
<button type="submit">Save</button>
</form>